<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="SongMusic" table="song_table">  
    <id name="id_song">  
     <generator class="assigned"></generator>  
    </id>  
            
    <property name="name_song"></property>  
    <property name="author_song"></property> 
    <property name="date_add_song"></property>
    <property name="public_song"></property> 
            
  </class>  
            
 </hibernate-mapping>  